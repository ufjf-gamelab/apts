{
  "devDependencies": {
    "prettier": "^3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "turbo": "^2.4.4"
  },
  "name": "quati",
  "packageManager": "pnpm@9.0.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo lint",
    "preview": "pnpm --filter=node preview",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch"
  }
}
