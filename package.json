{
  "devDependencies": {
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "prettier": "^3.7.4",
    "turbo": "^2.7.3",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": "=22"
  },
  "name": "quati",
  "packageManager": "pnpm@10.25.0",
  "pnpm": {
    "overrides": {
      "@ts-graphviz/adapter": "3.0.4",
      "@ts-graphviz/ast": "3.0.4",
      "@ts-graphviz/common": "3.0.4",
      "@ts-graphviz/core": "3.0.4",
      "ts-graphviz": "3.0.4"
    },
    "updateConfig": {
      "ignoreDependencies": [
        "@ts-graphviz/*",
        "ts-graphviz"
      ]
    },
    "onlyBuiltDependencies": [
      "@tensorflow/tfjs-node",
      "core-js",
      "edgedriver",
      "esbuild",
      "geckodriver"
    ]
  },
  "private": true,
  "scripts": {
    "build": "turbo run build --continue",
    "check-types": "turbo run check-types --continue",
    "dev": "turbo run dev --continue",
    "format": "turbo run format --continue",
    "lint": "turbo run lint --continue",
    "lint:fix": "turbo run lint:fix --continue",
    "preview": "turbo run preview --filter=quati-node --continue --",
    "test": "turbo run test --continue",
    "test:watch": "turbo run test:watch --continue"
  },
  "type": "module"
}
